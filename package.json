{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "export": "pnpm build && pnpm run copy-php && pnpm run move-output",
    "copy-php": "cp php-api/mail.php .output/public/mail.php",
    "move-output": "rm -rf output && mkdir output && cp -r .output/public/* output/ && cp -r php-api output/php",
    "clean": "rm -rf .nuxt .output dist app.vue.js",
    "check": "pnpm install && echo install success && pnpm lint && pnpm build && pnpm test && echo check success",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "pnpm lint:tsc && pnpm lint:vue && pnpm lint:eslint && echo lint success",
    "lint:eslint": "eslint --cache --ext .ts,.vue .",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:tsc": "tsc --noEmit && echo lint:tsc success",
    "lint:vue": "vue-tsc --noEmit && echo lint:vue success",
    "test": "echo no tests yet ^^"
  },
  "dependencies": {
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/i18n": "^8.5.6",
    "@nuxtjs/supabase": "^1.5.0",
    "@nuxtjs/tailwindcss": "^6.13.2",
    "@pinia/nuxt": "0.10.1",
    "@vee-validate/nuxt": "^4.15.0",
    "nuxt": "^3.16.2",
    "nuxt-icon": "^0.6.10",
    "nuxt-svgo": "^4.0.17",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@nuxtjs/sitemap": "^7.2.10",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@types/node": "^22.13.17",
    "@volar/typescript": "2.4.12",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.23.0",
    "eslint-config-hardcore": "^45.8.0",
    "eslint-formatter-summary": "^1.1.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "typescript": "^5.3.3",
    "vue-tsc": "1.8.8"
  },
  "version": "0.0.0"
}